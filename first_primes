import math
def prime(n):
    result=True 
    for i in range(2,int(math.sqrt(n)+1):
         if(n%i==0):
             result=False
             break
    return result
 
 
def first_m_primes(n):
    count=0
    i=2
    pl=[]
    while(count<n):
        if(prime(i)):
           count=count+1
           pl.append(i)
           i=i+1
    return pl
         
    
        
    
